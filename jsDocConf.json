{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.(jsdoc|js|mjs)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
	"opts": {
		"destination": "./jsDocOutput/",
		"recusive": true
	}
}

